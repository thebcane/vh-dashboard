    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
